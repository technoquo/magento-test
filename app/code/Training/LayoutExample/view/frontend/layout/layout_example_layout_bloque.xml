<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<head>
 <title>Bloque en Magento2</title>
</head>
  <body>
   <referenceContainer name="content">     
     
      <block name="layout_example" class="Training\LayoutExample\Block\Layout\Bloque" template="Training_LayoutExample::content.phtml">
        <block name="layout_note" class="Training\LayoutExample\Block\Layout\Note" template="Training_LayoutExample::note.phtml" />
      </block>
      <block name="template_contacto" class="Magento\Cms\Block\Block">
        <arguments>
           <argument name="block_id" xsi:type="string">template_contacto</argument>
        </arguments>
      </block>
     </referenceContainer>
    </body>

</page>
